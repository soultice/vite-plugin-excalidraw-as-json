  },
  "homepage": "https://github.com/soultice/vite-plugin-excalidraw-as-json#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.4",
    "rollup": "^2.77.2",
    "tslib": "^2.4.0",
    "tsup": "^6.6.3",
    "typescript": "^4.7.4"
  },
